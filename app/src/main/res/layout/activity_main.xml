<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="@dimen/cardview_default_radius"
    android:visibility="visible"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/main_label_principal"
        android:layout_width="137dp"
        android:layout_height="wrap_content"
        android:accessibilityLiveRegion="polite"
        android:lineSpacingExtra="8sp"
        android:rotationX="0"
        android:text="olÃ¡,enfermeira"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="?android:attr/candidatesTextStyleSpans"
        tools:visibility="visible" />

    <Button
        android:id="@+id/button"
        android:layout_width="139dp"
        android:layout_height="77dp"
        android:onClick="metodo"
        android:text="Salvar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_label_principal" />

    <EditText
        android:id="@+id/main_input_name"
        android:layout_width="268dp"
        android:layout_height="41dp"
        android:layout_marginBottom="96dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.417"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>